<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  <title>어디서 뭐하지</title>

  <link href="./css/historyc.css" rel="stylesheet">
</head>

<body>
  <div id="jb-container">
    <div id="jb-header">History</div>
    <hr class="line" />
    <div id="jb-content">
      <table class="m_table" id="meet_table" align="left">
        <thead>
          <th>번호</th>
          <th>모임 날짜</th>
          <th>모임 이름</th>
          <th>자세히</th>
          <tr>
            <td><a id="meet_num1">01</a></td>
            <td><a id="meet_date1">2019-12-25</a></td>
            <td><a id="meet_name1">초등학교 동창 모임</a></td>
            <td><button class="more" id="meet1">more</button></td>
          </tr>
          <tr>
            <td><a id="meet_num2">02</a></td>
            <td><a id="meet_date2">2020-01-01</a></td>
            <td><a id="meet_name2">동국대학교 컴퓨터공학과 학생회</a></td>
            <td><button class="more" id="meet2">more</button></td>
          </tr>
          <tr>
            <td><a id="meet_num3">03</a></td>
            <td><a id="meet_date3">2019-12-31</a></td>
            <td><a id="meet_name3">동국대학교 공과대학 풍물패 한소리 송년회</a></td>
            <td><button class="more" id="meet3">more</button></td>
            <td class="no"> <a class="no" id="meet_attend3">오유민</a>
              <a class="no" id="meet_places3">카페 이리로</a></td>
          </tr>
        </thead>
        <tbody id="meet_body"></tbody>
      </table>
    </div>
    <hr class="line" />
    <button class="go_main" onclick="location.href='/'">
      main
    </button>
  </div>

  <!-- Modal -->
  <div class="more_full">
    <div class="more_box">
      <div class="out">
        <button class="x"><img src="./img/x.png" /> </button>
      </div>

      <div class="details">
        <!-- 내용 -->
        <div class="left">
          <div class="la">
            <label>모임 이름</label>
          </div>
          <hr />
          <div class="la">
            <label>모임 날짜</label>
          </div>
          <hr />
          <div class="la_name">
            <label>참석자 이름</label>
          </div>
          <hr />
          <div class="la_places">
            <label>모임 장소</label>
          </div>
        </div>
        <div class="middle">
        </div>
        <div class="right" id="meet_more">
          <div class="la">
            <label id="pname1">모임 이름</label>
          </div>
          <hr />
          <div class="la">
            <label id="pdate1">모임 날짜</label>
          </div>
          <hr />
          <div class="la_name">
            <label id="pattend1">참석자 이름</label>
          </div>
          <hr />
          <div class="la_places">
            <label id="pplaces1">모임 장소</label>
          </div>

        </div>
      </div>

      <div class="buttons">
        <button class="de_btn" id="meet_remove1">
          삭제
        </button>
        <button class="de_btn" id="meet_share1">
          공유
        </button>
      </div>
    </div>
  </div>

  <script type="text/javascript" src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
  <script>
    $(document).ready(function() {
      $('#meet1').on('click', function() {
        $('.more_full').toggleClass('open');
        return false;
      });
      $('#meet2').on('click', function() {
        $('.more_full').toggleClass('open');
        return false;
      });
      $('#meet3').on('click', function() {
        $('.more_full').toggleClass('open');
        return false;
      });
      $('#meet4').on('click', function() {
        $('.more_full').toggleClass('open');
        return false;
      });
      $('#meet5').on('click', function() {
        $('.more_full').toggleClass('open');
        return false;
      });
      $('#meet6').on('click', function() {
        $('.more_full').toggleClass('open');
        return false;
      });
      $('#meet7').on('click', function() {
        $('.more_full').toggleClass('open');
        return false;
      });
      $('#meet8').on('click', function() {
        $('.more_full').toggleClass('open');
        return false;
      });
      $('#meet9').on('click', function() {
        $('.more_full').toggleClass('open');
        return false;
      });
      $('#meet10').on('click', function() {
        $('.more_full').toggleClass('open');
        return false;
      });

    });
    $(document).ready(function() {
      $('.x').on('click', function() {
        $('.more_full').toggleClass('close');
        return false;
      });
    });
  </script>
</body>

</html>

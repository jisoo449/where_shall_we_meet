<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title> 상세 보기 </title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <style type="text/css">
    body {
      background-image: url('header-bg.jpg');
    }

    #jb-container {
      width: 1000px;
      margin: 10px auto;
      padding: 20px;
      border: 1px solid #bcbcbc;
      background-color: rgb(45, 62, 80);
    }

    #jb-header {
      padding: 20px;
      margin-bottom: 20px;
      border: 1px solid #bcbcbc;
      background-color: rgb(45, 62, 80);
    }


    #jb-footer {
      clear: both;
      padding: 20px;
      border: 1px solid #bcbcbc;
      background-color: rgb(45, 62, 80);
    }

    @media (max-width: 480px) {
      #jb-container {
        width: auto;
      }

      #jb-content {
        float: none;
        width: auto;
      }

      #jb-sidebar {
        float: none;
        width: auto;
      }
    }
  </style>
</head>

<body>
  <font color='white'>
    <div id="jb-container">
      <div id="jb-header">
        <h1>History</h1>
      </div>
      <div id="jb-content">
        <h2>From database</h2>
        <div>
          <%= rows %>
        </div>
      </div>
      <script>
        function doDataBinding(data, databindings){
        for(var prop in databindings)
            if( databindings.hasOwnProperty(prop) )
                data = data.split('${'+prop+'}').join(databindings[prop]);
        return data;
      </script>
      <p>
        <button type="button" onclick="try{alert(삭제하시겠습니까? ');}catch(e){}return(false)">
          remove
        </button>
        <input type="text" id="ShareUrl">
        <span class="btn-type1"><button OnClick="javascript:CopyUrlToClipboard()">URL 복사</button></span>
        <div id="jb-footer">
          <button type="button" onclick="location.href='index'">
            <p>
              뒤로가기
            </p>
          </button>
        </div>
    </div>
  </font>
</body>
</html>
